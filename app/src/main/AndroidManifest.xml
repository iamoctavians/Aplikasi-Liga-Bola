<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pcsbambang.app.plesbol">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.pcsbambang.app.plesbol.activity.TeamDetailsActivity"></activity>
        <activity android:name="com.pcsbambang.app.plesbol.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.pcsbambang.app.plesbol.activity.EventDetailsActivity" />
        <activity android:name="com.pcsbambang.app.plesbol.activity.LeagueDetailActivity" />
        <activity android:name="com.pcsbambang.app.plesbol.activity.MainActivity" />
    </application>

</manifest>